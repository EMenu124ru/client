    "build-storybook": "build-storybook",
    "type-check": "tsc-files"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint:fix --cache"
    ]
  },
  "dependencies": {
