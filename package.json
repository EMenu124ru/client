    "@mui/material": "^5.10.10",
    "@mui/x-date-pickers": "^6.4.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@reduxjs/toolkit": "^1.6.2",
    "@svgr/webpack": "^5.5.0",
    "axios": "^1.1.3",
    "dotenv": "^10.0.0",
    "formik": "^2.2.9",
    "immer": "^9.0.12",
    "moment": "^2.29.4",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^7.2.6",
    "react-refresh": "^0.11.0",
    "react-router-dom": "^6.3.0",
    "tsc-files": "1.1.4"
